{
  "expo": {
    "name": "weather-app",
    "slug": "weather-app",
    "sdkVersion": "42.0.0",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icons/adaptive-icon.png",
    "splash": {
      "image": "./src/assets/icons/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#E1DFD8"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icons/adaptive-icon.png",
        "backgroundColor": "#E1DFD8"
      },
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./src/assets/icon/favicon.png"
    }
  }
}
